{
  "name": "functionfoundry",
  "version": "0.10.14",
  "description": "JavaScript functions like spreadsheet",
  "main": "lib/functionfoundry.js",
  "jsnext:main": "lib/functionfoundry.es6",
  "scripts": {
    "test": "babel-tape-runner test/*.js | faucet",
    "docs": "docco src/*.js",
    "test:docs": "docco --output test_docs test/*.js",
    "clean": "rm -rf lib && rm -rf dist && mkdir lib && mkdir dist",
    "build:dist": "browserify ./browserify.js > dist/functionfoundry.js && browserify -g uglifyify ./browserify.js > dist/functionfoundry.min.js",
    "build:jsnext": "rollup ./src/index.js > lib/functionfoundry.es6",
    "build:js": "babel lib/functionfoundry.es6 > lib/functionfoundry.js",
    "build": "npm run clean && npm run build:jsnext && npm run build:js && npm run build:dist && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FunctionFoundry/functionfoundry.git"
  },
  "keywords": [
    "Spreadsheets",
    "JavaScript"
  ],
  "author": "Peter Moresi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FunctionFoundry/functionfoundry/issues"
  },
  "homepage": "https://github.com/FunctionFoundry/functionfoundry#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-tape-runner": "^2.0.1",
    "browserify": "^13.0.0",
    "docco": "^0.7.0",
    "eslint": "^1.8.0",
    "faucet": "0.0.1",
    "rollup": "^0.26.2",
    "tape": "^4.5.1",
    "uglifyify": "^3.0.1"
  }
}
